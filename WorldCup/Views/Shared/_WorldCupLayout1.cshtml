<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />

   @* @Scripts.Render("~/bundles/bootstrapStyles")*@

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />

    <link href="~/Content/vendor/fontawesome/fontawesome-all.min.css" rel="stylesheet" />
    <link href="~/Content/vendor/fontawesome/fontawesome.css" rel="stylesheet" />

    <link href="~/Content/Shared/theme.css" rel="stylesheet" />
    <link href="~/Content/Shared/layout.min.css" rel="stylesheet" />

    @RenderSection("Meta")
</head>
<body>

    <!-- Wrapper -->
    <!-- Header -->
    <header id="header">
        <h1 id="layout-logo">
            <a href="#">
                WorldCup 2018
                <img src="~/Resources/Images/worldCup2018.png" />
            </a>
        </h1>
        <nav class="links">
            <ul>
                <li><a href="#">Lorem</a></li>
                <li><a href="#">Ipsum</a></li>
                <li><a href="#">Feugiat</a></li>
                <li><a href="#">Tempus</a></li>
                <li><a href="#">Adipiscing</a></li>
            </ul>
        </nav>
        <nav class="main">
            <ul>
                <li class="search">
                    <a href="#search"> Search</a>
                    <form id="search" method="get" action="#">
                        <input type="text" name="query" placeholder="Search" />
                    </form>
                </li>
                <li class="menu">
                    <a href="#menu"> Menu</a>
                </li>
            </ul>
        </nav>
    </header>

    <!-- Menu -->
    <section id="menu">

        <!-- Search -->
        <section>
            <form class="search" method="get" action="#">
                <input type="text" name="query" placeholder="Search" />
            </form>
        </section>

        <!-- Links -->
        <section>
            <ul class="links">
                <li>
                    <a href="#">
                        <h3>Lorem ipsum</h3>
                        <p>Feugiat tempus veroeros dolor</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <h3>Dolor sit amet</h3>
                        <p>Sed vitae justo condimentum</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <h3>Feugiat veroeros</h3>
                        <p>Phasellus sed ultricies mi congue</p>
                    </a>
                </li>
                <li>
                    <a href="#">
                        <h3>Etiam sed consequat</h3>
                        <p>Porta lectus amet ultricies</p>
                    </a>
                </li>
            </ul>
        </section>

        <!-- Actions -->
        <section>
            <ul class="actions vertical">
                <li><a href="#" class="button big fit">Log In</a></li>
            </ul>
        </section>

    </section>
    <div class="container"
         ng-app="worldcup">
        @RenderBody()
    </div>

    <!-- Scripts -->
    @Scripts.Render("~/bundles/jquery")

    @Scripts.Render("~/bundles/bootstrapScripts")
    <script src="~/Scripts/bootstrap.min.js"></script>
    <!-- AngularJS -->
    <script src="~/Scripts/node_modules/angular/angular_1.6.9.min.js"></script>
    <script>
        var APPNAME = "worldcup";

        (function () {
            "use strict"
            angular.module(APPNAME, []);
        })();
    </script>

    @RenderSection("Scripts")

</body>
</html>